<%- if controller_name != 'sessions' %>
  <%= link_to "Iniciar sesion", new_session_path(resource_name), class: "btn btn-secondary" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Registrarme", new_registration_path(resource_name), class: "btn btn-secondary" %><br />
<% end %>